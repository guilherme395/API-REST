{
  "scripts": {
    "dev": "nodemon src/server.cjs",
    "start": "node src/server.cjs",
    "migrate:create": "npx knex migrate:make --knexfile src/models/connection/knexfile.cjs",
    "migrate:up": "npx knex migrate:latest --knexfile src/models/connection/knexfile.cjs",
    "migrate:rollback": "npx knex migrate:rollback --knexfile src/models/connection/knexfile.cjs",
    "migrate:down": "npx knex migrate:down --knexfile src/models/connection/knexfile.cjs",
    "seed:make": "npx knex seed:make --knexfile src/models/connection/knexfile.cjs",
    "seed:run": "npx knex seed:run --knexfile src/models/connection/knexfile.cjs"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.0",
    "express": "^4.21.2",
    "jsonwebtoken": "^9.0.2",
    "knex": "^3.1.0",
    "morgan": "^1.10.0",
    "mysql2": "^3.13.0"
  },
  "devDependencies": {
    "nodemon": "^3.1.9"
  }
}
